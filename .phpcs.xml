<?xml version="1.0"?>
<ruleset>
	<rule ref="./vendor/mediawiki/mediawiki-codesniffer/MediaWiki">
		<exclude name="MediaWiki.Commenting.ClassAnnotations.UnrecognizedAnnotation" />
	</rule>
	<rule ref="MediaWiki.Usage.ExtendClassUsage">
		<properties>
			<property name="nonConfigGlobals[]" value="$wgWmincProjects,$wgWmincPref,$wgWmincProjectSite" />
		</properties>
	</rule>
	<file>.</file>
	<arg name="extensions" value="php" />
	<arg name="encoding" value="UTF-8" />
</ruleset>
